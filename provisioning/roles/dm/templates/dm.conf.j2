auth {
    oauth {
        google {
            key = "{{ google_key }}"
            secret = "{{ google_secret }}"
        }
        github {
            key = "{{ github_key }}"
            secret = "{{ github_secret }}"
        }
        independent {
            uri = "{{ doorkeeper_provider_base_url }}"
            key = "{{ doorkeeper_provider_key }}"
            secret = "{{ doorkeeper_provider_secret }}"
            description = "{{ doorkeeper_provider_description }}"
        }
    }
}
superuser {
    id = "{{ superuser_id }}"
}
hide {
    uris = ""
}
